<form action="/signUp" method="post">
    <input type="text" name="name">
    <input type="text" name="email">
    <input type="password" name="password">
    <button type="submit">Submit</button>
</form>

{{#if error }}
    <p>{{error}}</p>
{{else if successe }}
    <p>successe</p>
{{else}}
{{/if}}